<?xml version="1.0" encoding="utf-8"?>
<config type="module" version="1.5.0">
	<name>File multiple upload</name>
	<fields count="2">
		<field1>
			<maxlength>255</maxlength>
			<size>25</size>
			<type>file_multiple</type>
		</field1>
		<field2>
			<label>Files directory</label>
			<format>none</format>
			<cat>parameter</cat>
		</field2>
	</fields>
	<help>Upload multiple files at once. The 'directory' can be left blank.</help>
</config>