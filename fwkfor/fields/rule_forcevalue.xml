<?xml version="1.0" encoding="utf-8"?>
<config type="rule" version="1.5.0">
	<name>Force value</name>
	<fields count="2">
		<field1>
			<name>type</name>
			<label>Type</label>
			<type>select</type>
			<values type="multi">
				<option value="function">Function</option>
				<option value="constant">Constant</option>
				<option value="variable">Variable</option>
				<option value="method">Class method</option>
			</values>
		</field1>
		<field2>
			<name>value</name>
			<label>Value</label>
			<size>25</size>
			<maxlength>50</maxlength>
		</field2>
	</fields>
</config>
