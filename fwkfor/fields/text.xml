<?xml version="1.0" encoding="utf-8"?>
<config type="module" version="1.5.0">
	<name>Text free</name>
	<settings>
		<fieldattributes>
			<size>yes</size>
			<maxlength>yes</maxlength>
			<regex>yes</regex>
		</fieldattributes>
	</settings>
	<fields count="1">
		<field1>
			<name>freetext</name>
			<maxlength>255</maxlength>
			<size>50</size>
			<format>tinytext</format>
		</field1>
	</fields>
	<help>Input field with variable number of characters.</help>
</config>