<?xml version="1.0" encoding="utf-8"?>
<config type="module" version="1.5.0">
	<name>Currency</name>
	<fields count="1">
		<field1>
			<name>currency</name>
			<format>varchar(3)</format>
			<default>USD</default>
			<type>select</type>
			<values type="multi">
				<option1 value="USD">USD</option1>
				<option2 value="GBP">GBP</option2>
				<option3 value="EUR">EUR</option3>
				<option4 value="AUD">AUD</option4>
				<option5 value="CAD">CAD</option5>
				<option6 value="NZD">NZD</option6>
				<option7 value="JPY">JPY</option7>
				<option8 value="MXN">MXN</option8>
				<option9 value="SEK">SEK</option9>
				<option10 value="DKK">DKK</option10>
				<option11 value="CHF">CHF</option11>
				<option12 value="NOK">NOK</option12>
				<option12 value="RUB">RUB</option12>
				<option13 value="ZAR">ZAR</option13>
			</values>
		</field1>
	</fields>
	<help>Dropdown of common currencies.</help>
</config>
