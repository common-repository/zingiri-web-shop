<?xml version="1.0" encoding="utf-8"?>
<config type="module" version="1.5.0">
	<name>Date format</name>
	<fields count="1">
		<field1>
			<name>format</name>
			<format>varchar(15)</format>
			<type>select</type>
			<values type="multi">
				<option VALUE="d-m-Y @ G:i">dd-mm-YYYY @ 23:59</option>
				<option VALUE="m-d-Y @ G:i">mm-dd-YYYY @ 23:59</option>
			</values>
		</field1>
	</fields>
</config>